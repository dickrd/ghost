{
  "masterUrl": "http://127.0.0.1:666",
  "maxSleepMs": 60000,
  "page": 3,
  "engines": [
    {
      "name": "news.baidu.com",
      "queryUrl": "http://news.baidu.com/ns?word=%s&pn=%d0",
      "charset": "utf8",
      "config": {
        "method": "jsoup",
        "type": "link",
        "data": ".result > h3 > a"
      }
    },
    {
      "name": "news.sogou.com",
      "queryUrl": "http://news.sogou.com/news?query=%s&page=%d",
      "charset": "utf8",
      "config": {
        "method": "jsoup",
        "type": "link",
        "data": ".vrTitle > a"
      }
    },
    {
      "name": "weibo.com",
      "queryUrl": "http://s.weibo.com/weibo/%s",
      "charset": "utf8",
      "config": {
        "method": "regex",
        "type": "link",
        "data": "http:\\\\/\\\\/weibo\\.com\\\\/[0-9]+\\\\/[0-9a-zA-Z]+\\?refer_flag=[0-9]+_",
        "strip": "\\\\"
      }
    }
  ]
}